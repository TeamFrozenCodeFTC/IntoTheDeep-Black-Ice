instead of
if (Target.x > Target.previousX) {
                    return -x <= slope * (y - Target.y) - Target.x;
                }
                if (Target.x < Target.previousX) { // (Target.x < Target.previousX)
                    return x <= slope * (-y + Target.y) + Target.x;
                }
